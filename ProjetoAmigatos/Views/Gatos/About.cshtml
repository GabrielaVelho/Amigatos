@model IEnumerable<ProjetoAmigatos.ViewModels.RacaAbout>

@{
    ViewBag.Title = "About";
}

<br /> <br /> <br /> <br />
<img src="~/Images/amigatos.jpg" />

<h2>About</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NomeRaca)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Qtd)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TotalAdocoes)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.numMasculino)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.numFeminino)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NomeRaca)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Qtd)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TotalAdocoes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.numMasculino)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.numFeminino)
        </td>
    </tr>
}

</table>
